#include <stdio.h>
Main () {
     Int n1 , n2;   //variable declaration
      
     Printf("enter the first number:");
     Scanf ("%d", &n1);

     Printf("enter the second number:");
     Scanf("%d", &n1);

     // Process

     sum = nl + n2;

     diff= nl - n2;

     prod= nl * n2;

     quo= nl / n2;

     rem= n1 % n2;
   
    //Display outputs 
   printf("\nSUM\t\t: %d", n1+n2);
   printf("\nDIFFERENCE\t: %d", n1-n2);
   printf("PRODUCT\t\t: %d", n1*n2);
   printf("\nQUOTIENT\t: %d", nl/n2);
   printf("\nREMAINDER\t: %d", n1%n2);

}

